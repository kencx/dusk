package partials

import (
	"dusk"
)

templ Library(books dusk.Books, err error) {
	<div class="library__card-view">
		if err == dusk.ErrNoRows {
			<p>No items found.</p>
		} else if books != nil {
			for _, book := range books {
				@bookCard(book)
			}
		} else {
			<p>Something went wrong, please try again.</p>
		}
	</div>
}
